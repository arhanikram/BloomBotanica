<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.UserPlantProfileActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/user_plant_profile_toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        app:navigationIcon="@drawable/baseline_arrow_back_24"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/userplant_nickname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UserPlantProfileActivity"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />
<!--    text gets changed in UserPlantProfileActivity.java so hardcode is okay  -->

</androidx.constraintlayout.widget.ConstraintLayout>